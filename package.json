{
  "name": "task-tracker",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "lint": "npx prettier --check --ignore-unknown \"src/**/*.{js,jsx}\"",
    "lint:fix": "npx prettier --write --ignore-unknown \"src/**/*.{js,jsx}\"",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "coverage": "vitest run --coverage"
  },
  "lint-staged": {
    "**/*.js": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "bootstrap": "^5.2.3",
    "classnames": "^2.3.2",
    "framer-motion": "^8.5.5",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.6.2",
    "sass": "^1.57.1",
    "styled-components": "^5.3.6",
    "vite-plugin-babel-macros": "^1.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^3.0.0",
    "babel-loader": "^8.3.0",
    "git-commit-msg-linter": "^4.5.0",
    "husky": "^8.0.0",
    "jsdom": "^21.0.0",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.3",
    "prettier-plugin-organize-imports": "^3.2.2",
    "validate-branch-name": "^1.3.0",
    "vite": "^4.0.0",
    "vitest": "^0.28.4"
  }
}
